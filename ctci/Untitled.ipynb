    "def super_digit(num, sumnum=0):\n",
    "    sumnum += int(num[0])\n",
    "\n",
    "    if len(num) == 1:\n",
    "        if sumnum > 9 :\n",
    "            return super_digit(str(sumnum), sumnum=0)\n",
    "        else:\n",
    "            return sumnum\n",
    "    return super_digit(num[1:], sumnum)"
   ]
